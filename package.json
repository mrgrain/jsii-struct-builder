  "author": {
    "name": "Momo Kornher",
    "url": "https://moritzkornher.de",
    "organization": false
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2",
    "@types/jest": "^29.5.14",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "commit-and-tag-version": "^12",
    "constructs": "^10.0.0",
    "eslint": "^9",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "jest-junit": "^16",
    "mrpj": "^0.1.83",
    "projen": "^0.91.13",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "projen": "x.x.x"
  },
  "dependencies": {
    "@jsii/spec": "^1.108.0"
